{
  "rewrites": [
    {
      "source": "/",
      "destination": "/web/index.html"
    },
    {
      "source": "/index.html",
      "destination": "/web/index.html"
    },
    {
      "source": "/dashboard",
      "destination": "/web/index.html"
    },
    {
      "source": "/style.css",
      "destination": "/web/style.css"
    },
    {
      "source": "/main.js",
      "destination": "/web/main.js"
    },
    {
      "source": "/api/data",
      "destination": "/data/test_results.json"
    },
    {
      "source": "/test_results.json",
      "destination": "/data/test_results.json"
    }
  ],
  "headers": [
    {
      "source": "/(.*).json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Cache-Control",
          "value": "s-maxage=300, stale-while-revalidate"
        }
      ]
    },
    {
      "source": "/(.*).css",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/css"
        },
        {
          "key": "Cache-Control",
          "value": "s-maxage=86400, stale-while-revalidate"
        }
      ]
    },
    {
      "source": "/(.*).js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        },
        {
          "key": "Cache-Control",
          "value": "s-maxage=86400, stale-while-revalidate"
        }
      ]
    }
  ]
}
